{
    "manifest_version": 3,
    "name": "Password-Manager",
    "version": "1.0",
    "description": "A simple password manager to store all your user-credentials",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "/icons/logo16.jpg",
        "48": "/icons/logo48.jpg",
        "128": "/icons/logo128.jpg"
      }
    },
    "permissions": ["storage","tabs"]
}
  